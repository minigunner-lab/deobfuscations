local DrRayLibrary = loadstring(game:HttpGet("https://raw.githubusercontent.com/AZYsGithub/DrRay-UI-Library/main/DrRay.lua"))()
local window = DrRayLibrary:Load("Tower and Troll", "Default")

local tab = DrRayLibrary.newTab("Menu", "ImageIdHere")

tab.newButton("Get Slap", "Prints Hello!", function()
    -- Generated by @TheMugenKing
local hrp = game:GetService('Players').LocalPlayer.Character:WaitForChild('HumanoidRootPart')
local SpawnList = {
    Vector3.new(-15.283447265625, 104.43826293945312, 50.3544921875)
}
for _, pos in ipairs(SpawnList) do
    task.wait(0.4)
    hrp.CFrame = CFrame.new(pos)
end

end)

tab.newButton("Spam", "Prints Hello!", function()
local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local player = Players.LocalPlayer

-- Posisi teleportasi
local SpawnList = {
    Vector3.new(-50.000003814697266, 229.5, -13.000003814697266),
    Vector3.new(-50.000003814697266, 229.5, 12.999996185302734)
}

local function startTeleportLoop(char)
    local hrp = char:WaitForChild("HumanoidRootPart")
    local hum = char:FindFirstChildOfClass("Humanoid")
    local index = 1

    -- Pastikan tidak bind dua kali
    RunService:UnbindFromRenderStep("TeleportSpeedLoop")

    RunService:BindToRenderStep("TeleportSpeedLoop", Enum.RenderPriority.Camera.Value + 1, function()
        if not char or not char.Parent or not hum or hum.Health <= 0 or not hrp then
            RunService:UnbindFromRenderStep("TeleportSpeedLoop")
            return
        end

        hrp.CFrame = CFrame.new(SpawnList[index])
        index += 1
        if index > #SpawnList then
            index = 1
        end
    end)
end

-- Pertama kali
if player.Character then
    startTeleportLoop(player.Character)
end

-- Ulangi jika respawn
player.CharacterAdded:Connect(function(char)
    startTeleportLoop(char)
end)

end)

tab.newButton("End", "Prints Hello!", function()
    -- Generated by @TheMugenKing
local hrp = game:GetService('Players').LocalPlayer.Character:WaitForChild('HumanoidRootPart')
local SpawnList = {
    Vector3.new(4.999995231628418, 543.5, -285.5)
}
for _, pos in ipairs(SpawnList) do
    task.wait(0.4)
    hrp.CFrame = CFrame.new(pos)
end

end)
